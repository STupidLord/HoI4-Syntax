                    "beginCaptures": {
                        "1": {
                            "name": "punctuation.definition.variable.hoi4"
                        }
                    },
                    "end": "\\s",
                    "contentName": "variable.other.hoi4",
                    "patterns": [
                        {
                            "include": "#constants"
                        }
                    ]
                }
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "constant.rhs.tag.hoi4",
					"match": "\\b([A-Z][A-Z\\d][A-Z\\d])\\b"
				},
                {
                    "name": "constant.prefdef.hoi4",
                    "match": "(@\\w+)"
                },
                {
                    "name": "constant.language.hoi4",
                    "match": "(yes|no)+"
                },
                {
                    "name": "constant.numeric.hoi4",
                    "match": "-?\\b(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))\\b"
                },
                {
                    "name": "constant.rhs.hoi4",
                    "match": "(\\d|\\.)+"
                },
                {
                    "name": "constant.rhs.string.hoi4",
                    "match": "(\\w|\\.)+"
                }
            ]
		},
		"id": {
			"patterns": [
                {
                    "name": "meta.id.hoi4",
                    "match": "^\\s*[^@]?((id)\\s[=]\\s([A-Za-z_]+(.)([0-9]+)))",
                    "captures": {
                        "1": {
                            "name": "meta.id.identifier.txt"
                        },
                        "2": {
                            "name": "keyword.other.id.txt"
                        },
                        "3": {
                            "name": "entity.name.type.id.txt"
                        },
                        "4": {
                            "name": "entity.name.type.period.txt"
                        },
                        "5": {
                            "name": "entity.name.type.number.txt"
                        }
                    }
                }
            ]
		},
		"block": {
			"patterns": [
				{
					"name": "block.hoi4",
					"begin": "(?<==)\\s*{",
					"beginCaptures": {
						"0": {
							"name": "block.begin.hoi4"
						}
					},
					"end": "}",
					"endCaptures": {
						"0": {
							"name": "block.end.hoi4"
						}
					},
					"patterns": [
                        {
                            "include": "#code"
                        }
                    ]
				}
			]
		},
		"code": {
			"patterns": [
				{
                    "include": "#block"
                },
                {
                    "include": "#comment"
                },
				{
                    "include": "#id"
                },
                {
                    "include": "#variables"
                },
                {
                    "name": "filter.focus.hoi4",
                    "match": "\\b(FOCUS_FILTER_[A-Z]+)\\b"
                },
                {
                    "name": "stateprov.hoi4",
                    "match": "\\b(\\d+)\\b"
                }
			]
		},
		"strings": {
			"name": "string.quoted.double.hoi4",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.hoi4",
					"match": "\\\\."
				}
			]
		}
	}
}